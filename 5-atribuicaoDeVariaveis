console.log("Trabalhando com Atribuição de Variáveis" );

const nome = "Ricardo";
const sobrenome = "Bugan";

console.log(nome, sobrenome);
console.log(`meu nome é ${nome} ${sobrenome}`);

let contador = 0;
contador = contador +1;

const nomeCompleto = nome + sobrenome;
console.log(nomeCompleto);

let idade;
idade = 26;
idade = idade+1;
console.log(idade)
